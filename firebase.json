{
  "hosting": {
    "public": ".",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "/admin",
        "destination": "/index.html",
        "type": 200
      },
      {
        "source": "/installer/**",
        "destination": "/index.html",
        "type": 200
      },
      {
        "source": "**",
        "destination": "/index.html",
        "type": 200
      }
    ]
  }
}
